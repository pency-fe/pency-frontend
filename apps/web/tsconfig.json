{
  "extends": "@pency/typescript-config/nextjs.json",
  "compilerOptions": {
    "baseUrl": "app/",
    "paths": {
      "@/_route/*": ["_route/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", "next.config.mjs", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
